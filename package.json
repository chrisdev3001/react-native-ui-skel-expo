{
  "name": "react-native-ui-skel-expo",
  "version": "1.2.3",
  "description": "A simple React Native component that implements a Skeleton UI using Expo.",
  "main": "./package/src/index.js",
  "types": "./package/src/index.d.ts",
  "files": [
    "./package/src",
    "!**/*.test.*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
